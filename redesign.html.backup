                          <div class="flex justify-center mb-4 group-hover:scale-110 transition-all duration-300">
                            <svg class="w-12 h-12 text-purple-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4M7.835 4.697a3.42 3.42 0 001.946-.806 3.42 3.42 0 014.438 0 3.42 3.42 0 001.946.806 3.42 3.42 0 013.138 3.138 3.42 3.42 0 00.806 1.946 3.42 3.42 0 010 4.438 3.42 3.42 0 00-.806 1.946 3.42 3.42 0 01-3.138 3.138 3.42 3.42 0 00-1.946.806 3.42 3.42 0 01-4.438 0 3.42 3.42 0 00-1.946-.806 3.42 3.42 0 01-3.138-3.138 3.42 3.42 0 00-.806-1.946 3.42 3.42 0 010-4.438 3.42 3.42 0 00.806-1.946 3.42 3.42 0 01"/>
                            </svg>
                          </div>
                          <div class="font-bold text-black">CTF Competitor</div>
                          <div class="text-sm text-gray-600">Black Hat, Local Events</div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </section>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Scripts -->
    <script>
      // Initialize Lucide icons
      lucide.createIcons();
      
      // Register GSAP plugins
      gsap.registerPlugin(ScrollTrigger);
      
      // Initialize pill animation for redesign
      function initRedesignPillAnimation() {
        const container = document.getElementById('about-pill-container');
        const triangle = document.getElementById('about-triangle');
        const openBracket = document.getElementById('about-open-bracket');
        const closeBracket = document.getElementById('about-close-bracket');
        const text = document.getElementById('about-pill-text');
        
        if (!container || !triangle || !openBracket || !closeBracket || !text) return;
        
        // Set initial state
        gsap.set([triangle, openBracket, closeBracket, text], {
          opacity: 0,
          x: 0
        });
        
        // Animate on load
        const tl = gsap.timeline({ delay: 0.5 });
        
        tl.to([triangle, openBracket], {
          opacity: 1,
          duration: 0.3,
          stagger: 0.1,
          ease: "power2.out"
        });
        
        const textWidth = text.offsetWidth;
        tl.set(closeBracket, {
          x: -(textWidth + 8),
          opacity: 1
        });
        
        tl.to(closeBracket, {
          x: 0,
          duration: 0.4,
          ease: "power2.inOut"
        });
        
        tl.to(text, {
          opacity: 1,
          duration: 0.06,
          repeat: 5,
          yoyo: true,
          ease: "power2.inOut"
        }, "-=0.2");
        
        tl.set(text, {
          opacity: 1
        });
      }
      
      // Tab switching functionality
      function switchTab(tabName) {
        // Update pill text
        const pillText = document.getElementById('pill-text-dynamic');
        if (pillText) {
          pillText.textContent = tabName.toUpperCase();
        }
        
        // Update tab buttons
        const aboutTab = document.getElementById('about-tab');
        const experienceTab = document.getElementById('experience-tab');
        const aboutContent = document.getElementById('about-content');
        const experienceContent = document.getElementById('experience-content');
        
        if (tabName === 'about') {
          aboutTab.classList.add('active');
          experienceTab.classList.remove('active');
          aboutContent.classList.remove('hidden');
          experienceContent.classList.add('hidden');
        } else {
          experienceTab.classList.add('active');
          aboutTab.classList.remove('active');
          experienceContent.classList.remove('hidden');
          aboutContent.classList.add('hidden');
          
          // Initialize rainbow underline animation when experience tab is shown
          setTimeout(() => {
            initRainbowUnderlineAnimation();
          }, 200);
        }
        
        // Re-initialize icons for new content
        setTimeout(() => {
          lucide.createIcons();
        }, 100);
        
        console.log(`üéØ Switched to ${tabName} tab`);
      }
      
      // Rainbow underline animation
      function initRainbowUnderlineAnimation() {
        // Clear any existing ScrollTriggers for this element
        ScrollTrigger.getAll().forEach(trigger => {
          if (trigger.vars.trigger && trigger.vars.trigger.classList && trigger.vars.trigger.classList.contains('rainbow-underline')) {
            trigger.kill();
          }
        });
        
        const underline = document.querySelector('.rainbow-underline');
        if (!underline) {
          console.log('‚ùå Rainbow underline element not found');
          return;
        }
        
        console.log('‚úÖ Found rainbow underline element:', underline);
        
        // Refresh ScrollTrigger to recalculate positions
        ScrollTrigger.refresh();
        
        // Create ScrollTrigger for the underline animation
        ScrollTrigger.create({
          trigger: underline,
          start: "top 80%",
          end: "bottom 20%",
          onEnter: () => {
            console.log('üéØ Underline entering view - adding animation');
            underline.classList.add('animate-slide');
            
            // Start pulse effect after slide animation completes
            setTimeout(() => {
              underline.classList.add('pulse-active');
              
              // Remove pulse class after animation completes to allow re-triggering
              setTimeout(() => {
                underline.classList.remove('pulse-active');
              }, 2000);
            }, 1200); // Wait for slide animation to complete
          },
          onLeave: () => {
            console.log('üéØ Underline leaving view - removing animation');
            underline.classList.remove('animate-slide');
          },
          onEnterBack: () => {
            console.log('üéØ Underline entering back - adding animation');
            underline.classList.add('animate-slide');
            
            // Start pulse effect after slide animation completes
            setTimeout(() => {
              underline.classList.add('pulse-active');
              
              // Remove pulse class after animation completes to allow re-triggering
              setTimeout(() => {
                underline.classList.remove('pulse-active');
              }, 2000);
            }, 1200); // Wait for slide animation to complete
          },
          onLeaveBack: () => {
            console.log('üéØ Underline leaving back - removing animation');
            underline.classList.remove('animate-slide');
          },
          markers: false // Set to true for debugging
        });
        
        console.log('üåà Rainbow underline animation initialized with ScrollTrigger');
      }
      
      // Animate rainbow underline from left to right on first view
      function animateRainbowUnderlineOnce() {
        const underline = document.getElementById('rainbow-underline');
        if (!underline) return;
        // Remove class in case it's present
        underline.classList.remove('animate-slide');
        // Force initial state
        underline.style.transform = 'scaleX(0)';
        underline.style.transformOrigin = 'left';
        underline.style.transition = 'transform 1.2s cubic-bezier(0.4, 0, 0.2, 1)';
        // Animate in after short delay by adding the class
        setTimeout(() => {
          underline.classList.add('animate-slide');
        }, 400);
      }

      document.addEventListener('DOMContentLoaded', () => {
        lucide.createIcons();
        initSimpleParallax();
      });
      
      // Simple parallax: Hero scrolls down, Next section slides up covering it
      function initSimpleParallax() {
        const hero = document.getElementById('hero-section');
        const next = document.getElementById('next-section');
        
        if (!hero || !next) return;
        
        // Hero section scrolls down (moves with scroll, goes out of view naturally)
        gsap.to(hero, {
          y: 300, // Move down as we scroll
          scrollTrigger: {
            trigger: hero,
            start: 'top top',
            end: 'bottom top',
            scrub: true
          }
        });
        
        // Next section slides up to cover the hero
        gsap.from(next, {
          y: 100, // Starts slightly below, moves up
          scrollTrigger: {
            trigger: hero,
            start: 'top top',
            end: 'bottom top',
            scrub: true
          }
        });
        
        console.log('‚úÖ Simple parallax initialized');
      }
      
      console.log('üé® Parallax ready!');
    </script>
  </body>
</html>