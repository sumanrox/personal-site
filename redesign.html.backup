      /* Tab functionality styles */
      .tab-button {
        background: white;
        color: #374151;
        border: none;
        cursor: pointer;
      }
      
      .tab-button:hover {
        background: #f9fafb;
        color: black;
      }
      
      .tab-button.active {
        background: black;
        color: white;
      }
      
      .tab-content {
        opacity: 1;
        transition: opacity 0.3s ease-in-out;
      }
      
      .tab-content.hidden {
        display: none;
        opacity: 0;
      }
      
      /* Rainbow animation for experience section */
      @keyframes rainbow {
        0% { background-position: 0% 50%; }
        50% { background-position: 100% 50%; }
        100% { background-position: 0% 50%; }
      }

      .animate-rainbow {
        animation: rainbow 3s ease-in-out infinite;
      }
      
      /* Rainbow underline slide animation */
      .rainbow-underline {
        transform: scaleX(0);
        transform-origin: left;
        transition: transform 1.2s cubic-bezier(0.4, 0, 0.2, 1);
        position: relative;
        overflow: hidden;
      }
      
      .rainbow-underline.animate-slide {
        transform: scaleX(1);
      }
      
      /* Rainbow pulse effect */
      .rainbow-underline::before {
        content: '';
        position: absolute;
        top: -4px;
        left: -100%;
        width: 150%;
        height: calc(100% + 8px);
        background: linear-gradient(
          90deg,
          transparent 0%,
          rgba(255, 255, 255, 0.9) 10%,
          rgba(255, 255, 255, 1) 50%,
          rgba(255, 255, 255, 0.9) 90%,
          transparent 100%
        );
        opacity: 0;
        transition: opacity 0.3s ease;
        filter: blur(2px);
        border-radius: 4px;
      }
      
      .rainbow-underline.pulse-active::before {
        opacity: 1;
        animation: rainbow-pulse 2s ease-in-out;
      }
      
      @keyframes rainbow-pulse {
        0% {
          left: -150%;
          background: linear-gradient(
            90deg,
            transparent 0%,
            rgba(239, 68, 68, 0.3) 5%,
            rgba(239, 68, 68, 1) 30%,
            rgba(239, 68, 68, 1) 70%,
